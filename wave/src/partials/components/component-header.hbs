<!-- header begin -->
<header>
    <div class="uk-section uk-padding-remove-vertical{{#ifpage 'index' 'homepage2' 'homepage3' 'homepage4'}} in-header-inverse{{/ifpage}} {{#unlesspage 'index' 'homepage2' 'homepage3' 'homepage4'}}in-header-inner uk-background-cover uk-background-top-center" style="background-image: url({{root}}img/in-wave-background-1.png);{{/unlesspage}}">
        <nav class="uk-navbar-container uk-navbar-transparent" data-uk-sticky="show-on-up: true; top: 80; animation: uk-animation-fade">
            <div class="uk-container" data-uk-navbar>
                <div class="uk-navbar-left uk-width-expand uk-flex uk-flex-between">
                    <a class="uk-navbar-item uk-logo" href="{{root}}index.html" {{#ifpage 'index' 'homepage2' 'homepage3' 'homepage4'}}data-logo-inverse="filename: header-logo-WeY2ag.svg"{{/ifpage}} {{#unlesspage 'index' 'homepage2' 'homepage3' 'homepage4'}}data-logo-inverse="filename: header-logo-WeY2ag.svg; sticky-only: true"{{/unlesspage}}>
                        <img src="{{root}}{{component.header.siteLogo.src}}" alt="logo" width="{{component.header.siteLogo.width}}" height="{{component.header.siteLogo.height}}">
                    </a>
                    <ul class="uk-navbar-nav uk-visible@m">
                        {{#each navigation}}
                        <li><a href="{{@root.root}}{{link}}">{{title}}{{#if dropdown.child}}<span data-uk-navbar-parent-icon></span>{{/if}}</a>
                            {{#unless dropdown.childText}}
                            {{#if dropdown.child}}
                            <div class="uk-navbar-dropdown">
                                <ul class="uk-nav uk-navbar-dropdown-nav">
                                    {{#each dropdown.child}}
                                    <li><a href="{{@root.root}}{{link}}">{{title}}{{#if icon}}<i class="fas {{icon}} fa-sm"></i>{{/if}}</a></li>
                                    {{/each}}
                                </ul>
                            </div>
                            {{/if}}
                            {{else}}
                            <div class="uk-navbar-dropdown uk-navbar-dropdown-width-2">
                                <div class="uk-navbar-dropdown-grid uk-child-width-1-2" data-uk-grid>
                                    <div>
                                        <ul class="uk-nav uk-navbar-dropdown-nav">
                                            {{#each dropdown.child}}
                                            <li><a href="{{@root.root}}{{link}}">{{title}}{{#if icon}}<i class="fas {{icon}} fa-sm"></i>{{/if}}</a></li>
                                            {{/each}}
                                        </ul>
                                    </div>
                                    <div>
                                        <ul class="uk-nav uk-navbar-dropdown-nav">
                                            <li><a class="uk-disabled" href="#">{{dropdown.childText}}</a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            {{/unless}}
                        </li>
                        {{/each}}
                    </ul>
                </div>
                <div class="uk-navbar-right uk-width-auto">
                    <div class="uk-navbar-item uk-visible@m">
                        <div class="in-optional-nav">
                            <a href="{{#if component.header.signin.external}}{{component.header.signin.url}}{{else}}{{root}}{{component.header.signin.url}}{{/if}}" class="uk-button uk-button-text"><i class="fas fa-user-circle uk-margin-small-right"></i>{{component.header.signin.title}}</a>
                            <a href="{{#if component.header.signup.external}}{{component.header.signup.url}}{{else}}{{root}}{{component.header.signup.url}}{{/if}}" class="uk-button uk-button-primary uk-button-small uk-border-pill">{{component.header.signup.title}}</a>
                        </div>
                    </div>
                </div>
            </div>
        </nav>
        <div class="uk-container">
            <div class="uk-grid">
                <div class="uk-width-1-1">
                    <div class="uk-card uk-card-secondary uk-card-small uk-card-body uk-border-rounded">
                        <div class="uk-grid uk-text-small" data-uk-grid>
                            <div class="uk-width-3-4@m uk-visible@m">
                                <p>Trading involves substantial risk and may result in the loss of your invested/greater that your invested capital, respectively.</p>
                            </div>
                            <div class="uk-width-expand@m uk-text-center uk-text-right@m">
                                <a class="uk-margin-right" href="#"><i class="fas fa-comment-alt uk-margin-small-right"></i>Live Chat</a>
                                <a href="#"><i class="fas fa-phone-alt uk-margin-small-right uk-margin-small-left"></i>1-800-123-4567</a>
                            </div>
                        </div>
                    </div>
                </div>
                {{#if breadcrumb}}
                {{> component-breadcrumb }}
                {{/if}}
            </div>
        </div>
    </div>  
</header>
<!-- header end -->